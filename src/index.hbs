<body class="scroll-assist" style="margin-top: -70px;">
{{> navbar}}
<div class="main-container">
    {{> slider}}
      <section>
        <div class="container">
            <div class="row">
                <div class="col-sm-12 text-center">
                    <h1 class="uppercase mb16">Live Courses</h1>
                    <p class="lead mb64">
                        Live courses for all stages, with equal focus on theory and implementation</p>
                </div>
            </div>
            <a id="register-advance" class="in-page-link"></a>
            {{> coursesRow courses=liveCoursesRow1}}
            {{> coursesRow courses=liveCoursesRow2}}
            {{> coursesRow courses=liveCoursesRow3}}

        </div>
    </section>
    
    <section>
        <div class="container">
            <div class="row">
                <div class="col-sm-12 text-center">
                    <h1 class="uppercase mb16">Online Courses</h1>
                    <p class="lead mb64">
                        The interactive learning for aspiring and experienced developers</p>
                </div>
            </div>
            {{> coursesRow courses=onlineCoursesRow1}}
            {{> coursesRow courses=onlineCoursesRow2}}
            {{> coursesRow courses=onlineCoursesRow3}}
            {{!-- {{> coursesRow courses=onlineCoursesRow4}} --}}
        </div>
    </section>
    {{> numbers}}
    <div class="hidden-xs">
        {{> team}}
    </div>
    <div class="visible-xs">
        <section>
            <div class="row">
                <div class="col-sm-12 text-center">
                    <div style="padding: 10px">
                        <h1 class="uppercase mb16">Our Team</h1>
                        <p class="lead mb64"> The backbone of Coding Blocks is the team of enthusiastic and talented
                            mentors. Our best-in-class team comprises of mentors with vast industrial experience, people
                            who have published papers in international journals, and developers associated with GSoC and
                            Google Code-in.</p>
                    </div>
                </div>
            </div>
        </section>
    </div>
    {{> review-course-section}}
    {{> map}}
    {{> footer}}
</div>
